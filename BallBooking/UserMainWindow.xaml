<Window x:Class="BallBooking.UserMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BallBooking"
        mc:Ignorable="d"
        Title="UserMainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Bóng đá" HorizontalAlignment="Left" Margin="362,10,0,0" VerticalAlignment="Top" FontFamily="Times New Roman" FontSize="24" FontWeight="Bold"/>
        <Button x:Name="Booking" Content="Xem lịch đặt" HorizontalAlignment="Left" Margin="32,75,0,0" VerticalAlignment="Top" Height="93" Width="330" FontFamily="Times New Roman" FontSize="22" FontWeight="Bold" Background="#FFC4DC83" BorderBrush="#FFA37E7E" Foreground="#FF504A80" Click="Booking_Click" />
        <Button x:Name="Prifile" Content="Hồ sơ cá nhân" HorizontalAlignment="Left" Margin="441,75,0,0" VerticalAlignment="Top" Height="93" Width="330" FontFamily="Times New Roman" FontSize="22" FontWeight="Bold" Background="#FFC4DC83" BorderBrush="#FFA37E7E" Foreground="#FF504A80" Click="Prifile_Click" />
    </Grid>
</Window>
